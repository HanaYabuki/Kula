class Father {
    name() {
        print this.name;
    }

    do() {
        print this.name + " do sth.";
    }
    
    constructor() {
        this.name = "Jack";
    }
}

class Son : Father {
    name() {
        print "Son";
    }

    constructor() {
        this.name = "Jackson";
    }
}

father := Father();
son := Son();

father.do();
son.do();


